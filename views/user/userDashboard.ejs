								<div class="user-img">
									<img src="/images/avatar/img-5.jpg" alt="">
									<div class="img-add">													
										<input type="file" id="file">
										<label for="file"><i class="uil uil-camera-plus"></i></label>
									</div>
								</div>
								<h4><%= user.name %></h4>
								<p><%= user.phone ? 'user.phone ' : 'user.email'%><a href="#"><i class="uil uil-edit"></i></a></p>

							</div>
						</div>
					</div>
				</div>
			</div>	
			<div class="">
